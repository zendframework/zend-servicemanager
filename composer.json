{
    "name": "Mxcframework/Mxc-servicemanager",
    "description": "Factory-Driven Dependency Injection Container",
    "license": "BSD-3-Clause",
    "keywords": [
        "di",
        "dic",
        "dependency-injection",
        "psr-11",
        "zf",
        "Mxcframework",
        "servicemanager",
        "service-manager"
    ],
    "support": {
        "docs": "https://docs.Mxcframework.com/Mxc-servicemanager/",
        "issues": "https://github.com/Mxcframework/Mxc-servicemanager/issues",
        "source": "https://github.com/Mxcframework/Mxc-servicemanager",
        "rss": "https://github.com/Mxcframework/Mxc-servicemanager/releases.atom",
        "slack": "https://Mxcframework-slack.herokuapp.com",
        "forum": "https://discourse.Mxcframework.com/c/questions/components"
    },
    "require": {
        "php": "^7.1",
        "container-interop/container-interop": "^1.2",
        "psr/container": "^1.0",
        "Mxcframework/Mxc-stdlib": "^3.1"
    },
    "require-dev": {
        "mikey179/vfsStream": "^1.6.4",
        "ocramius/proxy-manager": "^2.1.1",
        "phpbench/phpbench": "^0.13.0",
        "phpunit/phpunit": "^6.4.4",
        "Mxcframework/Mxc-coding-standard": "~1.0.0"
    },
    "provide": {
        "container-interop/container-interop-implementation": "^1.2",
        "psr/container-implementation": "^1.0"
    },
    "suggest": {
        "ocramius/proxy-manager": "ProxyManager ^2.1.1 to handle lazy initialization of services",
        "Mxcframework/Mxc-stdlib": "Mxc-stdlib ^2.7.7 | ^3.1 if you wish to use the MergeReplaceKey or MergeRemoveKey features in Config instances"
    },
    "autoload": {
        "psr-4": {
            "Mxc\\ServiceManager\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MxcTest\\ServiceManager\\": "test/",
            "MxcBench\\ServiceManager\\": "benchmarks/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.3-dev",
            "dev-develop": "4.0-dev"
        }
    },
    "bin": [
        "bin/generate-deps-for-config-factory",
        "bin/generate-factory-for-class"
    ],
    "scripts": {
        "check": [
            "@cs-check",
            "@test"
        ],
        "cs-check": "phpcs",
        "cs-fix": "phpcbf",
        "test": "phpunit --colors=always",
        "test-coverage": "phpunit --colors=always --coverage-clover clover.xml"
    }
}
